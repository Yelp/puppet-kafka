# Note: This file is managed by Puppet.

broker.id=<%= scope.lookupvar('kafka::server::broker_id') %>
log.dirs=<%= Array(scope.lookupvar('kafka::server::log_dirs')).join(',') %>
zookeeper.connect=<%= scope.lookupvar('kafka::server::zookeeper_connect') %>
broker.rack=<%= scope.lookupvar('kafka::server::broker_rack') %>

<% server_properties = scope.lookupvar('kafka::server::server_properties')
   server_properties.keys.sort.each do |key| -%>
<%= key %>=<%= server_properties[key] %>
<% end -%>
